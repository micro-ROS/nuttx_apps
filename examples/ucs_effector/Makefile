-include $(TOPDIR)/Make.defs

# uROS-publisher built-in application info

CONFIG_UCS_EFFECTOR_EXAMPLE_PRIORITY ?= SCHED_PRIORITY_DEFAULT
CONFIG_UCS_EFFECTOR_EXAMPLE_STACKSIZE ?= 65000

APPNAME = ucs_effector
PRIORITY = $(CONFIG_UCS_EFFECTOR_EXAMPLE_PRIORITY)
STACKSIZE = $(CONFIG_UCS_EFFECTOR_EXAMPLE_STACKSIZE)

# uROS-publisher Example

ASRCS =
CSRCS = init_effector_6lowpan.c
MAINSRC = ucs_effector_main.c

CONFIG_UCS_EFFECTOR_EXAMPLE_PROGNAME ?= ucs_effector$(EXEEXT)
PROGNAME = $(CONFIG_UCS_EFFECTOR_EXAMPLE_PROGNAME)
UCS_EFFECTOR_INCLUDES = $(shell find $(APPDIR)/microros/micro_ros_lib/include -type d -name include)
CFLAGS += ${shell $(INCDIR) $(INCDIROPT) "$(CC)" "$(UCS_EFFECTOR_INCLUDES)"} -std=c99

MODULE = CONFIG_UCS_EFFECTOR_EXAMPLE

include $(APPDIR)/Application.mk
