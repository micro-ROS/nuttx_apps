-include $(TOPDIR)/Make.defs

# uROS-publisher built-in application info

CONFIG_UCS_HIH6130_EXAMPLE_PRIORITY ?= SCHED_PRIORITY_DEFAULT
CONFIG_UCS_HIH6130_EXAMPLE_STACKSIZE ?= 65000

APPNAME = ucs_hih6130
PRIORITY = $(CONFIG_UCS_HIH6130_EXAMPLE_PRIORITY)
STACKSIZE = $(CONFIG_UCS_HIH6130_EXAMPLE_STACKSIZE)

# uROS-publisher Example

ASRCS =
CSRCS = init_hih_6lowpan.c
MAINSRC = ucs_hih6130_main.c

CONFIG_UCS_HIH6130_EXAMPLE_PROGNAME ?= ucs_hih6130$(EXEEXT)
PROGNAME = $(CONFIG_UCS_HIH6130_EXAMPLE_PROGNAME)
UCS_HIH6130_INCLUDES = $(shell find $(APPDIR)/microros/micro_ros_lib/include -type d -name include)
CFLAGS += ${shell $(INCDIR) $(INCDIROPT) "$(CC)" "$(UCS_HIH6130_INCLUDES)"} -std=c99

MODULE = CONFIG_UCS_HIH6130_EXAMPLE

include $(APPDIR)/Application.mk
